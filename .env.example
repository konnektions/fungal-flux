# Client-side environment variables (safe for browser via Vite)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Stripe publishable key (test or live)
VITE_STRIPE_PUBLISHABLE_KEY=

# Serverless function secrets (DO NOT prefix with VITE_)
# These are used by /api functions. In production, set in Vercel Project Settings.
# For local development with `vercel dev`, set them in a local .env (not committed).
STRIPE_SECRET_KEY=

# Optional: override currency (defaults to usd) used by the API
# CURRENCY=usd